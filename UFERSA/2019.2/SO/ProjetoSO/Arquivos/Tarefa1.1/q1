.data
	# Vetor 'dados' de bytes
	dados: .byte 72, 101, 108, 108, 111, 32, 87, 111, 114, 108, 100, 33, 10
	a:	.word	1
	b:	.word	2
	c:	.word	3
	d:	.word	4
	e:	.word	5

.text
func:	lw	$s1, b
	lw	$s2, c
	
	add	$s0, $s1, $s2	#b+c
	
	lw	$s1, d
	lw	$s2, e
	
	sub	$s1, $s1, $s2	#d-e
	sub	$s0, $s0, $s1	#b+c-(d-e)
	
	sw	$s0, a
	
	jr	$ra

.globl main #deve ser global
	main: 
	la $a0, dados($0)
	
	jal	func
	
	#imprimir inteiro
	li $v0, 4
	syscall

	# Terminando o programa
	li   $v0, 10
	syscall